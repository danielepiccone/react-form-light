{
  "name": "react-form-light",
  "version": "0.2.0",
  "description": "A minimal form framework for React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpiccone/react-form-light"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "compile": "babel src/ --out-dir lib --source-maps inline",
    "watch": "onchange 'src/**' -i -- npm-run-all compile",
    "lint": "standard",
    "jest": "jest src/ --watchAll",
    "test": "jest src/",
    "coverage": "jest src/ --coverage",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "jest-util": "^20.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.5",
    "sinon": "^2.3.2",
    "standard": "^8.0.0",
    "unexpected": "^10.29.0",
    "unexpected-react": "^4.1.0",
    "webpack": "^2.6.1"
  },
  "standard": {
    "ignore": [
      "node_modules",
      "lib"
    ]
  }
}
